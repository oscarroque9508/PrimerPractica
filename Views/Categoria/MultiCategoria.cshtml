@model List<Categoria>

    @{
        ViewData["Title"] = "Lista de Categorias";
    }

    <h1>@ViewData["Title"]</h1>

    <div class="container">
    <table class="table table-dark table-hover"> 
    <thead class="thead-light">
        <tr>
            <th scope="co1">ID</th>
            <th scope = "co2>">Nombre</th>
            <th scope = "co1"> Seleccion</th>
        </tr>
    </thead>
    <tbody class="">
        @foreach (var categoria in Model)
        {
            <partial name = "Categoria" model = "categoria"></partial>
        }
    </tbody>
    </table>
    </div>